<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Define Remotes -->
  <remote fetch="https://github.com/milanpreetkaur502/meta-de-recipe" name="meta-de"/>
  <remote fetch="https://github.com/mendersoftware" name="mender"/>
  
  <default sync-j="4" revision="main"/> 
  
  <!-- Define Projects -->
  <project name="meta-de" remote="meta-de-recipe" revision="main" path="layers"/>
  
  <project name="meta-mender" remote="mender" revision="dunfell" path="meta-mender"/>
  <project name="meta-mender-community" remote="mender" revision="dunfell" path="meta-mender-community">
  <copyfile dest="setup-environment" src="scripts/setup-environment"/>
  
  </project>
</manifest>

